  ZAddAssetAttr.ZCLOUDAVALANCHEPICKTYPE AS 'Cloud Avalanche Pick Type AddAssetAttr',
  ZAddAssetAttr.ZCLOUDRECOVERYSTATE AS 'Cloud Recovery State AddAssetAttr',
  ZAddAssetAttr.ZORIGINALRESOURCECHOICE AS 'Org Resource Choice AddAssetAttr',
  ZAddAssetAttr.ZSYNDICATIONIDENTIFIER AS 'Syndication Identifier ZAddAssetAttr',
  ZAddAssetAttr.ZSOURCEASSETFORDUPLICATIONSCOPEIDENTIFIER AS 'Source Asset For Duplication Scope ID ZAddAssetAttr',
  GenAlbum.Z_PK AS 'GenAlbum zPK',
  GenAlbum.ZKEYASSET AS 'Key Asset GenAlbum',
  GenAlbum.ZTERTIARYKEYASSET AS 'Tertiary Key Asset GenAlbum',
  GenAlbum.ZSECONDARYKEYASSET AS 'Secondary Key Asset GenAlbum',
  GenAlbum.ZCUSTOMKEYASSET AS 'Custom Key Asset GenAlbum',
  GenAlbum.ZSYNCEVENTORDERKEY AS 'Sync Event Order Key GenAlbum',
  GenAlbum.Z_FOK_PARENTFOLDER AS 'FOK Parent Folder GenAlbum',
  GenAlbum.ZCLOUDGUID AS 'Cloud GUID GenAlbum',
  GenAlbum.ZUUID AS 'GenAlbum UUID',
  GenAlbum.ZPROJECTRENDERUUID AS 'Project Render UUID GenAlbum',
  GenAlbum.ZKEYASSETFACEIDENTIFIER AS 'Key Asset Face ID GenAlbum',
  ZALBUMLIST.ZUUID AS 'Album List UUID',   
  CM.Z_PK AS 'Cloud Master zPK',
  CM.ZMEDIAMETADATA AS 'Media Metadata Cloud Master',
  CM.ZIMPORTSESSIONID AS 'Import Session ID Cloud Master',
  CM.ZORIGINATINGASSETIDENTIFIER AS 'Orginating Asset ID Cloud Master',
  CM.ZCLOUDMASTERGUID AS 'Cloud Master GUID Cloud Master',
  CM.ZPLACEHOLDERSTATE AS 'Place Holder State Cloud Master',
  CM.ZSOURCEMASTERFORDUPLICATIONIDENTIFIER AS 'Source Master For Duplication ID Key',
  CM.ZSOURCEMASTERFORDUPLICATIONSCOPEIDENTIFIER AS 'Source Master For Duplication Scope ID Key',
  CFE.Z_PK AS 'Cloud Feed Entry zPK',
  CFE.ZENTRYPRIORITYNUMBER AS 'Entry Priority Number CFE', 
  CFE.ZENTRYALBUMGUID AS 'Entry Album GUID CFE',
  CFE.ZENTRYINVITATIONRECORDGUID AS 'Entry Invitation Record GUID CFE',
  CFE.ZENTRYCLOUDASSETGUID AS 'Entry Cloud Asset GUID',
  CSC.Z_PK AS 'Cloud Shared Comment zPK',
  CSC.ZCLOUDFEEDCOMMENTENTRY AS 'Cloud Feed Comment Entry Key Cloud Shared Comment',
  CSC.ZCLOUDFEEDLIKECOMMENTENTRY AS 'Cloud Feed Like Comment Entry Key Cloud Shared Comment',
  CSC.ZCOMMENTEDASSET AS 'Commented Asset Key Cloud Shared Comment',
  CSC.ZLIKEDASSET AS 'Liked Asset Key Cloud Shared Comment',
  CSC.Z_FOK_CLOUDFEEDCOMMENTENTRY AS 'FOK Cloud Feed Comment Entry Key Cloud Shared Comment',
  CSC.Z_FOK_CLOUDFEEDLIKECOMMENTENTRY AS 'FOK Cloud Feed Like Comment Entry Key Cloud Shared Comment',
  CSC.ZCLOUDGUID AS 'Cloud GUID Cloud Shared Comment',
  CSAIR.Z_PK AS 'Cloud Shared Album Invite zPK',
  CSAIR.ZALBUM AS 'Album Key Cloud Share Album Invite Rec',
  CSAIR.Z_FOK_ALBUM AS 'FOK Album Key Cloud Share Album Invite Rec',
  CSAIR.ZALBUMGUID AS 'Album GUID Cloud Share Album Invite Rec',
  CSAIR.ZCLOUDGUID AS 'Cloud GUID Cloud Share Album Invite Rec',
  SHA.Z_PK AS 'Share zPK',
  SHA.ZUUID AS 'Share Album Share UUID SHA',
  SHAPart.Z_PK AS 'Share Participant zPK',
  SHAPart.ZROLE AS 'Role SHAPart',
  SHAPart.ZSHARE AS 'Share SHAPart',
  SHAPart.ZUSERIDENTIFIER AS 'User Identifier SHAPart', 
  SHAPart.ZUUID AS 'Share Participant UUID SHAPart',
  SHAPart.ZNAMECOMPONENTS AS 'Name Components SHAPart',
  SHAPart.Z51_SHARE AS '51 Share SHAPart',
  SHAPart.ZPERMISSION AS 'Premission SHAPart',
  SHAPart.ZISCURRENTUSER AS 'Is Current User SHAPart',
  CMMD.ZADDITIONALASSETATTRIBUTES AS 'AddAssetAtt Key Cloud Master MetaData',
  CMMD.Z_PK AS 'Cloud Master Media MetaData zPK',
  AssetDes.Z_PK AS 'Asset Description zPK',
  AssetDes.ZASSETATTRIBUTES AS 'Asset Attributes Asset Description Key',
  Z_27ASSETS.Z_27ALBUMS AS '27Asset.27Album Key = GenAlbum zPK',
  Z_27ASSETS.Z_3ASSETS AS '27Asset.3Assets Key = Asset zPK',
  Z_27ASSETS.Z_FOK_3ASSETS AS '27Asset.3Assets = unknown',  
  CAA.Z_PK AS 'Computed Asset Attributes zPK',
  IR.Z_PK AS 'Internal Resource zPK',
  IR.ZASSET AS 'Asset Key Internal Resource',
  MedAnalyAsset.ZASSET AS 'Asset Key MedAnalyAsset',
  MedAnalyAsset.Z_PK AS 'Media Analysis Asst Attributes zPK',
  MedAnalyAsset.ZMEDIAANALYSISVERSION AS 'Media Analysis Version MedAnalyAsset',
  UA.Z_PK AS 'Unmanaged Adjustment zPK',
  UA.ZASSETATTRIBUTES AS 'Asset Attributes Key UA',  
  UA.ZUUID AS 'Unmanaged Adjustment UUID UA',
  UA.ZOTHERADJUSTMENTSFINGERPRINT AS 'Other Adjustments Fingerprint UA',
  UA.ZSIMILARTOORIGINALADJUSTMENTSFINGERPRINT AS 'Similar Org Adjustments Fingerprint UA',
  DF.Z_PK AS 'Detected Face zPK',
  DF.ZASSET AS 'Asset Key Detected Face',
  DF.ZGROUPINGIDENTIFIER AS 'Grouping ID Detected Face',
  DF.ZMASTERIDENTIFIER AS 'Master ID Detected Face',
  DF.ZFACEALGORITHMVERSION AS 'Face Algorithm Version Detected Face',
  DFP.ZFACEPRINTVERSION AS 'Face Print Version DFP',
  FC.ZASSET AS 'Asset Key FC',
  SceneP.Z_PK AS 'Scene Print zPK',
  SceneP.ZADDITIONALASSETATTRIBUTES AS 'AddAssetAttr Key SceneP',
  MEM.Z_PK AS 'Memory zPK',
  MEM.ZKEYASSET AS 'Key Asset Key Memory',
  MEM.ZPHOTOSGRAPHVERSION AS 'Memory Photos Graph Version Memory',
  MemoryBECA.Z_3EXTENDEDCURATEDASSETS AS '3MemoryBECA Asset Key',
  MemoryBECA.Z_42MEMORIESBEINGEXTENDEDCURATEDASSETS AS '3MemoryBECA Memory Key',
  MOM.Z_PK AS 'Moment zPK',
  MOM.ZHIGHLIGHT AS 'Moment Highlight Key',
  MOM.ZUUID AS 'Moment UUID Moment',
  SUGG.Z_PK AS 'Suggestion zPK',
  SUGG.ZUUID AS 'Suggestion UUID SUGG',
  SUGG.ZVERSION AS 'Suggestion Version SUGG',
  PH.Z_PK AS 'Photos Highlight zPK',
  PH.ZPARENTPHOTOSHIGHLIGHT AS 'Parent Photos Highlight Key PH',
  PH.ZKEYASSET AS 'Key Asset Key PH',
  PH.ZDAYGROUPKEYASSET AS 'Day Group Key Asset Key PH',
  PH.ZPARENTDAYGROUPPHOTOSHIGHLIGHT AS 'Parent Day Group Photos Highlight Key Photo',
  PH.ZMONTHFIRSTASSET AS 'Month First Asset Key PH',
  PH.ZMONTHKEYASSET AS 'Month Key Asset Key PH',
  PH.ZYEARKEYASSET AS 'Year Key Asset Key PH',
  PH.ZUUID AS 'Photos Highlight UUID PH',
  PH.ZENRICHMENTVERSION AS 'Enrichment Version PH',
  PH.ZHIGHLIGHTVERSION AS 'Highlight Version PH',  
  PERREF.Z_PK AS 'Person Reference zPK',
  PERREF.ZASSETATTRIBUTES AS 'Asset Attributes Key PERREF',
  PERREF.ZPERSON AS 'Person Key PERREF',
  PERREF.ZPERSONUUID AS 'Person UUID PERREF',
  HEX(PERREF.ZPERSONDATA) AS 'Person Data PERREF',
  CRA.Z_PK AS 'Charter Recognitation Attributes CRA',
  CRA.ZMEDIAANALYSISASSETATTRIBUTES AS 'CRA Media Analysis Asset Attr Key',
  CRA.ZALGORITHMVERSION AS 'CRA Algorithm Version CRA',
  CRA.ZADJUSTMENTVERSION AS 'CRA Adjustment Version CRA',
  ExtAttr.ZASSET AS 'Asset Key ExtAttr',
  VSA.Z_PK AS 'Visual Search Attributes zPK VSA',
  VSA.ZALGORITHMVERSION AS 'Algorithm Version VSA',
  VSA.ZMEDIAANALYSISASSETATTRIBUTES AS 'Media Analysis Asset Attr VSA',
  VSA.ZADJUSTMENTVERSION AS 'Adjustment Version VSA',
  VSA.ZVISUALSEARCHDATA AS 'HEX Visual Search Data VSA'
FROM ZASSET
  LEFT JOIN ZADDITIONALASSETATTRIBUTES ZAddAssetAttr ON ZASSET.ZADDITIONALATTRIBUTES = ZAddAssetAttr.Z_PK 
  LEFT JOIN ZASSETDESCRIPTION AssetDes ON AssetDes.ZASSETATTRIBUTES = ZAddAssetAttr.Z_PK
  LEFT JOIN ZUNMANAGEDADJUSTMENT UA ON UA.Z_PK = ZAddAssetAttr.ZUNMANAGEDADJUSTMENT
  LEFT JOIN ZEXTENDEDATTRIBUTES ExtAttr ON ExtAttr.ZASSET = ZASSET.Z_PK
  LEFT JOIN Z_27ASSETS ON ZASSET.Z_PK = Z_27ASSETS.Z_3ASSETS
  LEFT JOIN ZGENERICALBUM GenAlbum ON GenAlbum.Z_PK = Z_27ASSETS.Z_27ALBUMS
  LEFT JOIN Z_26ALBUMLISTS ON GenAlbum.Z_PK = Z_26ALBUMLISTS.Z_26ALBUMS
  LEFT JOIN ZALBUMLIST ON ZALBUMLIST.Z_PK = Z_26ALBUMLISTS.Z_26ALBUMS
  LEFT JOIN ZSHARE SHA ON SHA.Z_PK = ZASSET.ZMOMENTSHARE
  LEFT JOIN ZSHAREPARTICIPANT SHAPart ON SHAPart.ZSHARE = SHA.Z_PK
  LEFT JOIN ZCLOUDMASTER CM ON CM.Z_PK = ZASSET.ZMASTER
  LEFT JOIN ZCLOUDMASTERMEDIAMETADATA CMMD ON CMMD.ZADDITIONALASSETATTRIBUTES = ZAddAssetAttr.ZMEDIAMETADATA AND CMMD.ZCLOUDMASTER = CM.Z_PK
  LEFT JOIN ZCLOUDFEEDENTRY CFE ON CFE.Z_PK = ZASSET.Z_PK
  LEFT JOIN ZCLOUDSHAREDALBUMINVITATIONRECORD CSAIR ON CSAIR.ZALBUM = GenAlbum.Z_PK
  LEFT JOIN ZCLOUDSHAREDCOMMENT CSC ON CSC.ZCOMMENTEDASSET = ZASSET.Z_PK
  LEFT JOIN ZCLOUDRESOURCE CLDRes ON CLDRes.ZCLOUDMASTER = CM.Z_PK
  LEFT JOIN ZDETECTEDFACE DF ON ZASSET.Z_PK = DF.ZASSET
  LEFT JOIN ZPERSON PER ON PER.Z_PK = DF.ZPERSON
  LEFT JOIN ZDETECTEDFACEPRINT DFP ON DFP.ZFACE = DF.Z_PK
  LEFT JOIN ZFACECROP FC ON PER.Z_PK = FC.ZPERSON
  LEFT JOIN ZUSERFEEDBACK UFB ON UFB.ZPERSON = PER.Z_PK
  LEFT JOIN ZPERSONREFERENCE PERREF ON PERREF.ZASSETATTRIBUTES = ZAddAssetAttr.Z_PK
  LEFT JOIN Z_3MEMORIESBEINGEXTENDEDCURATEDASSETS MemoryBECA ON MemoryBECA.Z_3EXTENDEDCURATEDASSETS = ZASSET.Z_PK
  LEFT JOIN ZMEMORY MEM ON MemoryBECA.Z_42MEMORIESBEINGEXTENDEDCURATEDASSETS = MEM.Z_PK
  LEFT JOIN ZPHOTOSHIGHLIGHT PH ON PH.Z_PK = ZASSET.ZHIGHLIGHTBEINGASSETS
  LEFT JOIN ZSUGGESTION SUGG ON SUGG.Z_PK = ZASSET.ZHIGHLIGHTBEINGASSETS
  LEFT JOIN ZMOMENT MOM ON MOM.ZHIGHLIGHT = PH.Z_PK
  LEFT JOIN ZSCENEPRINT SceneP ON SceneP.Z_PK = ZAddAssetAttr.ZSCENEPRINT
  LEFT JOIN ZCOMPUTEDASSETATTRIBUTES CAA ON CAA.Z_PK = ZASSET.Z_PK
  LEFT JOIN ZINTERNALRESOURCE IR ON IR.ZASSET = ZASSET.Z_PK
  LEFT JOIN ZASSETANALYSISSTATE AstAnalysisSt ON AstAnalysisSt.ZASSET = ZASSET.Z_PK
  LEFT JOIN ZMEDIAANALYSISASSETATTRIBUTES MedAnalyAsset ON MedAnalyAsset.ZASSET = ZASSET.ZMEDIAANALYSISATTRIBUTES
  LEFT JOIN ZVISUALSEARCHATTRIBUTES VSA ON VSA.Z_PK = MedAnalyAsset.ZVISUALSEARCHATTRIBUTES
  LEFT JOIN ZCHARACTERRECOGNITIONATTRIBUTES CRA ON CRA.Z_PK = MedAnalyAsset.ZCHARACTERRECOGNITIONATTRIBUTES
ORDER BY ZASSET.ZDATECREATED
