  Z_26ASSETS.Z_3ASSETS AS '26Asset.3Assets Key = Asset zPK',
  Z_26ASSETS.Z_FOK_3ASSETS AS '26Asset.3Assets = unknown',  
  CAA.Z_PK AS 'Computed Asset Attributes zPK',
  IR.Z_PK AS 'Internal Resource zPK',
  IR.ZCODEC AS 'Codec Key Internal Resource',
  IR.ZASSET AS 'Asset Key Internal Resource',
  IR.ZUNIFORMTYPEIDENTIFIER AS 'Uniform Type ID Key Internal Resource',
  MedAnalyAsset.ZASSET AS 'Asset Key MedAnalyAsset',
  MedAnalyAsset.Z_PK AS 'Media Analysis Asst Attributes zPK',
  ZCODEC.Z_PK AS 'Codec zPK',
  UA.Z_PK AS 'Unmanaged Adjustment zPK',
  UA.ZASSETATTRIBUTES AS 'Asset Attributes Key UA',  
  UA.ZUUID AS 'Unmanaged Adjustment UUID UA',
  UA.ZOTHERADJUSTMENTSFINGERPRINT AS 'Other Adjustments Fingerprint UA',
  UA.ZSIMILARTOORIGINALADJUSTMENTSFINGERPRINT AS 'Similar Org Adjustments Fingerprint UA',
  DF.Z_PK AS 'Detected Face zPK',
  DF.ZASSET AS 'Asset Key Detected Face',
  DF.ZGROUPINGIDENTIFIER AS 'Grouping ID Detected Face',
  DF.ZMASTERIDENTIFIER AS 'Master ID Detected Face',
  FC.ZASSET AS 'Asset Key FC',
  SceneP.Z_PK AS 'Scene Print zPK',
  SceneP.ZADDITIONALASSETATTRIBUTES AS 'AddAssetAttr Key SceneP',
  MEM.Z_PK AS 'Memory zPK',
  MEM.ZKEYASSET AS 'Key Asset Key Memory',
  MemoryBECA.Z_3EXTENDEDCURATEDASSETS AS '3MemoryBECA Asset Key',
  MemoryBECA.Z_40MEMORIESBEINGEXTENDEDCURATEDASSETS AS '3MemoryBECA Memory Key',
  MOM.Z_PK AS 'Moment zPK',
  MOM.ZHIGHLIGHT AS 'Moment Highlight Key',
  MOM.ZUUID AS 'Moment UUID Moment',
  MOMLis.Z_PK AS 'Moment List zPK',
  MOMLis.ZUUID AS 'Moment List UUID Moment List',  
  SUGG.Z_PK AS 'Suggestion zPK',
  SUGG.ZUUID AS 'Suggestion UUID SUGG',
  PH.Z_PK AS 'Photos Highlight zPK',
  PH.ZPARENTPHOTOSHIGHLIGHT AS 'Parent Photos Highlight Key PH',
  PH.ZKEYASSET AS 'Key Asset Key PH',
  PH.ZDAYGROUPKEYASSET AS 'Day Group Key Asset Key PH',
  PH.ZPARENTDAYGROUPPHOTOSHIGHLIGHT AS 'Parent Day Group Photos Highlight Key Photo',
  PH.ZMONTHFIRSTASSET AS 'Month First Asset Key PH',
  PH.ZMONTHKEYASSET AS 'Month Key Asset Key PH',
  PH.ZYEARKEYASSET AS 'Year Key Asset Key PH',
  PH.ZUUID AS 'Photos Highlight UUID PH',  
  PERREF.Z_PK AS 'Person Reference zPK',
  PERREF.ZASSETATTRIBUTES AS 'Asset Attributes Key PERREF',
  PERREF.ZPERSON AS 'Person Key PERREF',
  PERREF.ZPERSONUUID AS 'Person UUID PERREF',
  HEX(PERREF.ZPERSONDATA) AS 'Person Data PERREF',
  UniID.Z_PK AS 'Uniform Identifier zPK'
FROM ZASSET
  LEFT JOIN ZADDITIONALASSETATTRIBUTES ZAddAssetAttr ON ZASSET.ZADDITIONALATTRIBUTES = ZAddAssetAttr.Z_PK 
  LEFT JOIN ZASSETDESCRIPTION AssetDes ON AssetDes.ZASSETATTRIBUTES = ZAddAssetAttr.Z_PK
  LEFT JOIN ZUNMANAGEDADJUSTMENT UA ON UA.Z_PK = ZAddAssetAttr.ZUNMANAGEDADJUSTMENT
  LEFT JOIN Z_26ASSETS ON ZASSET.Z_PK = Z_26ASSETS.Z_3ASSETS
  LEFT JOIN ZGENERICALBUM GenAlbum ON GenAlbum.Z_PK = Z_26ASSETS.Z_26ALBUMS
  LEFT JOIN Z_25ALBUMLISTS ON GenAlbum.Z_PK = Z_25ALBUMLISTS.Z_25ALBUMS
  LEFT JOIN ZALBUMLIST ON ZALBUMLIST.Z_PK = Z_25ALBUMLISTS.Z_25ALBUMS
  LEFT JOIN ZCOMPUTEDASSETATTRIBUTES CAA ON CAA.Z_PK = ZASSET.Z_PK
  LEFT JOIN ZINTERNALRESOURCE IR ON IR.ZASSET = ZASSET.Z_PK
  LEFT JOIN ZUNIFORMTYPEIDENTIFIER UniID ON UniID.Z_PK = IR.ZUNIFORMTYPEIDENTIFIER
  LEFT JOIN ZCODEC ON ZCODEC.Z_PK = IR.ZCODEC
  LEFT JOIN ZCLOUDMASTER CM ON CM.Z_PK = ZASSET.ZMASTER
  LEFT JOIN ZCLOUDMASTERMEDIAMETADATA CMMD ON CMMD.ZADDITIONALASSETATTRIBUTES = ZAddAssetAttr.ZMEDIAMETADATA AND CMMD.ZCLOUDMASTER = CM.Z_PK
  LEFT JOIN ZCLOUDFEEDENTRY CFE ON CFE.Z_PK = ZASSET.Z_PK
  LEFT JOIN ZCLOUDSHAREDALBUMINVITATIONRECORD CSAIR ON CSAIR.ZALBUM = GenAlbum.Z_PK
  LEFT JOIN ZCLOUDSHAREDCOMMENT CSC ON CSC.ZCOMMENTEDASSET = ZASSET.Z_PK
  LEFT JOIN ZMEDIAANALYSISASSETATTRIBUTES MedAnalyAsset ON MedAnalyAsset.ZASSET = ZASSET.ZMEDIAANALYSISATTRIBUTES
  LEFT JOIN ZDETECTEDFACE DF ON ZASSET.Z_PK = DF.ZASSET
  LEFT JOIN ZPERSON PER ON PER.Z_PK = DF.ZPERSON
  LEFT JOIN ZDETECTEDFACEPRINT DFP ON DFP.ZFACE = DF.Z_PK
  LEFT JOIN ZFACECROP FC ON PER.Z_PK = FC.ZPERSON
  LEFT JOIN Z_3MEMORIESBEINGEXTENDEDCURATEDASSETS MemoryBECA ON MemoryBECA.Z_3EXTENDEDCURATEDASSETS = ZASSET.Z_PK
  LEFT JOIN ZMEMORY MEM ON MemoryBECA.Z_40MEMORIESBEINGEXTENDEDCURATEDASSETS = MEM.Z_PK
  LEFT JOIN ZPHOTOSHIGHLIGHT PH ON PH.Z_PK = ZASSET.ZHIGHLIGHTBEINGASSETS
  LEFT JOIN ZSUGGESTION SUGG ON SUGG.Z_PK = ZASSET.ZHIGHLIGHTBEINGASSETS
  LEFT JOIN ZMOMENT MOM ON MOM.ZHIGHLIGHT = PH.Z_PK
  LEFT JOIN ZMOMENTLIST MOMLis ON MOMLis.Z_PK = MOM.ZMEGAMOMENTLIST
  LEFT JOIN ZSHARE SHA ON SHA.Z_PK = ZASSET.ZMOMENTSHARE
  LEFT JOIN ZSCENEPRINT SceneP ON SceneP.Z_PK = ZAddAssetAttr.ZSCENEPRINT
  LEFT JOIN ZASSETANALYSISSTATE AstAnalysisSt ON AstAnalysisSt.ZASSET = ZASSET.Z_PK
  LEFT JOIN ZCLOUDRESOURCE CLDRes ON CLDRes.ZCLOUDMASTER = CM.Z_PK
  LEFT JOIN ZPERSONREFERENCE PERREF ON PERREF.ZASSETATTRIBUTES = ZAddAssetAttr.Z_PK
  LEFT JOIN ZSHAREPARTICIPANT SHAPart ON SHAPart.ZSHARE = SHA.Z_PK
ORDER BY ZASSET.ZDATECREATED
