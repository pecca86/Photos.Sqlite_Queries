    WHEN 5 THEN '5-Unknown'
    WHEN 6 THEN '6-Unknown'
    ELSE DF.ZHAIRCOLORTYPE
  END AS 'Hair Color Type Detected Face',
  CASE DF.ZHASSMILE
    WHEN 0 THEN 'No Smile'
    WHEN 1 THEN 'Yes Smile'
    ELSE DF.ZHASSMILE
  END AS 'Has Smile Detected Face',
  CASE DF.ZSMILETYPE
    WHEN 0 THEN '0-Unknown'
    WHEN 1 THEN 'No Teeth'
    WHEN 2 THEN 'Teeth Showing'
    ELSE DF.ZSMILETYPE
  END AS 'Smile Type Detected Face',
  CASE DF.ZLIPMAKEUPTYPE
    WHEN 0 THEN 'None'
    WHEN 1 THEN 'Lip Makeup Detected'
    ELSE DF.ZLIPMAKEUPTYPE
  END AS 'Lip Makeup Type Detected Face',
  DF.ZMANUAL AS 'Manual Detected Face',
  CASE DF.ZEYESSTATE
    WHEN 0 THEN 'Unknown'
    WHEN 1 THEN 'Closed'
    WHEN 2 THEN 'Open'
    ELSE DF.ZEYESSTATE
  END AS 'Eyes State Detected Face',
  CASE DF.ZISLEFTEYECLOSED
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
    ELSE DF.ZISLEFTEYECLOSED
  END AS 'Is Left Eye Closed Detected Face',
  CASE DF.ZISRIGHTEYECLOSED
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
    ELSE DF.ZISRIGHTEYECLOSED
  END AS 'Is Right Eye Closed Detected Face',
  CASE DF.ZGLASSESTYPE
    WHEN 0 THEN '0-Unknown'
    WHEN 3 THEN 'None'
    WHEN 2 THEN 'Sun Glasses'
    WHEN 1 THEN 'Eye Glasses'
    ELSE DF.ZGLASSESTYPE
  END AS 'Eye Glasses Type Detected Face',
  CASE DF.ZEYEMAKEUPTYPE
    WHEN 0 THEN 'No Eye Makeup'
    WHEN 1 THEN 'Eye Makeup Detected'
    ELSE DF.ZEYEMAKEUPTYPE
  END AS 'Eye Makeup Type Detected Face',
