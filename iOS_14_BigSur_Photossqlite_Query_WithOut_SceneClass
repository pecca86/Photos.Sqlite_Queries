    WHEN 0 THEN '0-Unknown'
    WHEN 1 THEN '1-None'
    WHEN 2 THEN '2-Beard'
    WHEN 3 THEN '3-Goatee'
    WHEN 4 THEN '4-Mustache'
    WHEN 5 THEN '5-Stubble'
    ELSE DF.ZFACIALHAIRTYPE
  END AS 'Facial Hair Type Detected Face',
