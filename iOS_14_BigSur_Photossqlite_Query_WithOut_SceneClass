--SQLite query for Photos.sqlite in iOS 14 and Big Sur
--Has everything possible that I could join except for Scene Classification. This one will also have a large result due to the number of Memories, Moments and Photo Highlights each asset will have.  
--By: Scott Koenig 
--Twitter: @Scott_Kjr
--Blog: https://theforensicscooter.com/
--Reminder to always test and validate!

SELECT
  CASE ZASSET.ZCOMPLETE
    WHEN 1 THEN 'Yes'
  END AS 'Asset Complete',
  ZASSET.Z_PK AS 'Asset zPK',
  ZAddAssetAttr.Z_PK AS 'AddAssetAttr zPK',
  DateTime(ZASSET.ZDATECREATED + 978307200, 'UNIXEPOCH') AS 'Date Created Asset',
  DateTime(ZASSET.ZDATECREATED + 978307200, 'UNIXEPOCH', 'localtime') AS 'Local Date Created Asset',
  ZAddAssetAttr.ZEXIFTIMESTAMPSTRING AS 'EXIF Timestamp String AddAssAttr',
  DateTime(ZASSET.ZMODIFICATIONDATE + 978307200, 'UNIXEPOCH') AS 'File Modification Date Asset',
  DateTime(CM.ZCREATIONDATE + 978307200, 'UNIXEPOCH') AS 'Creation Date Cloud Master',
  DateTime(IR.ZCLOUDMASTERDATECREATED + 978307200, 'UNIXEPOCH') AS 'Cloud Master Date Created Internal Resource',
  DateTime(ZASSET.ZADDEDDATE + 978307200, 'UNIXEPOCH') AS 'Add Date Asset',
  DateTime(ZASSET.ZLASTSHAREDDATE + 978307200, 'UNIXEPOCH') AS 'Last Shared Date Asset',
  DateTime(ZASSET.ZCLOUDBATCHPUBLISHDATE + 978307200, 'UNIXEPOCH') AS 'Cloud Batch Publish Date Asset',
  DateTime(ZASSET.ZCLOUDSERVERPUBLISHDATE + 978307200, 'UNIXEPOCH') AS 'Cloud Server Publish Date Asset',
  DateTime(CLDRes.ZDATECREATED + 978307200, 'UNIXEPOCH') AS 'Date Created CLDRes',
  DateTime(ZAddAssetAttr.ZLASTUPLOADATTEMPTDATE + 978307200, 'UNIXEPOCH') AS 'Last Upload Attempt Date AddAssetAttr',
  DateTime(CM.ZIMPORTDATE + 978307200, 'UNIXEPOCH') AS 'Import Date Cloud Master',
  DateTime(ZAddAssetAttr.ZALTERNATEIMPORTIMAGEDATE + 978307200, 'UNIXEPOCH') AS 'Alternate Import Image Date AddAssetAttr',
  DateTime(CLDRes.ZLASTPREFETCHDATE + 978307200, 'UNIXEPOCH') AS 'Last Prefetch Date CLDRes',
  IR.ZCLOUDPREFETCHCOUNT AS 'Cloud Prefetch Count Internal Resource',
  CASE IR.ZCLOUDLASTPREFETCHDATE
    WHEN 0 THEN 'NA'
	ELSE DateTime(IR.ZCLOUDLASTPREFETCHDATE + 978307200, 'UNIXEPOCH')
  END AS 'Cloud Last Prefetch Date Internal Resource', 
  ZAddAssetAttr.ZTIMEZONENAME AS 'Time Zone Name AddAssetAttr',
  ZAddAssetAttr.ZTIMEZONEOFFSET AS 'Time Zone Offset AddAssetAttr',
  ZAddAssetAttr.ZINFERREDTIMEZONEOFFSET AS 'Inferred Time Zone Offset AddAssetAttr',
  ZAddAssetAttr.ZCREATORBUNDLEID AS 'Creator Bundle ID AddAssetAttr',
  ZAddAssetAttr.ZIMPORTEDBYDISPLAYNAME AS 'Imported By Display Name AddAssetAttr',
  CM.ZIMPORTEDBYDISPLAYNAME AS 'Imported by Display Name Cloud Master',
  CM.ZIMPORTEDBYBUNDLEIDENTIFIER AS 'Imported by Bundle ID Cloud Master',
  ZAddAssetAttr.ZEDITORBUNDLEID AS 'Editor Bundle ID AddAssetAttr',
  CASE ZASSET.ZSAVEDASSETTYPE
    WHEN 0 THEN 'Saved via other source'
    WHEN 1 THEN '1-Unknown'
    WHEN 2 THEN '2-Unknown'
    WHEN 3 THEN 'DCIM APPLE'
    WHEN 4 THEN 'Photo Cloud Sharing Data'
    WHEN 5 THEN '5-Unknown'
    WHEN 6 THEN 'Cloud Photo Library Asset'
    WHEN 7 THEN '7-Unknown'
    WHEN 8 THEN 'Cloud Master Moment Asset'
    ELSE ZASSET.ZSAVEDASSETTYPE
  END AS 'Saved Asset Type',
  ZASSET.ZDIRECTORY AS 'Directory Asset',
  ZASSET.ZIMAGEREQUESTHINTS AS 'HEX Image Request Hints Asset',
  IR.ZDATASTOREKEYDATA AS 'HEX Data Store Key Data Internal Resource',
  CLDRes.ZFILEPATH AS 'File Path CLDRes',
  ZASSET.ZFILENAME AS 'Filename Asset',
  ZAddAssetAttr.ZORIGINALFILENAME AS 'Org Filename AddAssetAttr',
  CM.ZORIGINALFILENAME AS 'Org File Name Cloud Master',
  CASE ZAddAssetAttr.ZDESTINATIONASSETCOPYSTATE
    WHEN 0 THEN 'No Copy'
    WHEN 2 THEN 'Has A Copy'
  ELSE ZAddAssetAttr.ZDESTINATIONASSETCOPYSTATE
  END AS 'Destination Asset Copy State AddAssetAttr',
  CASE IR.ZDATASTORESUBTYPE 
    WHEN 0 THEN '0-Unknown'
    WHEN 1 THEN '1-Main Asset'
    WHEN 2 THEN '2-Photo with Adjustments'
    WHEN 3 THEN '3-Photo Cloud Sharing Data'
    WHEN 4 THEN '4-Has Uploaded to Cloud'
    WHEN 5 THEN '5-Has Uploaded to Cloud'
    WHEN 6 THEN '6-Unknown'
    WHEN 7 THEN '7-MP4-Unknown'
    WHEN 8 THEN '8-MP4-Unknown'
    WHEN 13 THEN '13-Unknown'
	WHEN 14 THEN '14-Unknown'
	WHEN 15 THEN '15-Has Markup and Adjustments'
	WHEN 16 THEN '16-Unknown'
	WHEN 17 THEN '17-Unknown'
    WHEN 18 THEN '18-Live Photo Video'
    WHEN 19 THEN '19-Live Photo with Adjustments'
	WHEN 22 THEN '22-Setting was Changed Prior to Capture'
    WHEN 26 THEN '26-Unknown'
    ELSE IR.ZDATASTORESUBTYPE
  END AS 'Data Store Sub Type Internal Resource',
  CASE IR.ZFILEID 
    WHEN -1 THEN 'Not Valid'
    ELSE IR.ZFILEID
  END AS 'File ID Key Internal Resource',
  IR.ZRECIPEID AS 'Recipe ID Key Internal Resource',
    CASE IR.ZVERSION
    WHEN 0 THEN '0-No Adjustments'
	WHEN 1 THEN '1-Unknown'
    WHEN 2 THEN '2-Adjustments'
    WHEN 3 THEN '3-Unknown'
    ELSE IR.ZVERSION
  END AS 'Version Internal Resource',
  ZASSET.ZORIGINALCOLORSPACE AS 'Original Color Space Asset',
  ZASSET.ZUNIFORMTYPEIDENTIFIER AS 'Uniform Type ID Asset',
  CM.ZUNIFORMTYPEIDENTIFIER AS 'Uniform Type ID Cloud Master',
  CLDRes.ZUNIFORMTYPEIDENTIFIER AS 'Unifor Type Identifier CLDRes', 
  UniID.ZIDENTIFIER AS 'Identifier UniID',
  CM.ZMEDIAMETADATATYPE AS 'Media Metadata Type Cloud Master',
  CASE UniID.ZCONFORMSTOIMAGE
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
    ELSE UniID.ZCONFORMSTOIMAGE
  END AS 'Conforms To Image UniID',
  CASE UniID.ZCONFORMSTOMOVIE
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
    ELSE UniID.ZCONFORMSTOMOVIE
  END AS 'Conforms To Movie UniID',
  CASE UniID.ZCONFORMSTORAWIMAGE
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
    ELSE UniID.ZCONFORMSTORAWIMAGE
  END AS 'Conforms To Raw Image UniID',
  ZAddAssetAttr.ZMEDIAMETADATATYPE AS 'Media Metadata Type AddAssetAttr',
  CASE ZASSET.ZCLOUDLOCALSTATE
    WHEN 0 THEN 'Not Synced / is in Cloud Share Album' 
    WHEN 1 THEN 'Synced with Cloud'
    ELSE ZASSET.ZCLOUDLOCALSTATE
  END AS 'Cloud Local State File Synced with Cloud Asset',
  CASE CM.ZCLOUDLOCALSTATE
    WHEN 0 THEN 'Not Synced with Cloud'
    WHEN 3 THEN 'Synced with Cloud'
    ELSE CM.ZCLOUDLOCALSTATE
  END AS 'Cloud Master Cloud Local State',
  CASE PER.ZCLOUDLOCALSTATE
    WHEN 0 THEN 'Not Synced with Cloud' 
    WHEN 1 THEN 'Synced with Cloud'
    ELSE PER.ZCLOUDLOCALSTATE
  END AS 'Person Cloud Local State',
  CASE DF.ZCLOUDLOCALSTATE
    WHEN 0 THEN 'Not Synced with Cloud' 
    WHEN 1 THEN 'Synced with Cloud'
    ELSE DF.ZCLOUDLOCALSTATE
  END AS 'Detected Face Cloud Local State DF',
  CASE FC.ZCLOUDLOCALSTATE 
    WHEN 0 THEN 'Not Synced with Cloud' 
    WHEN 1 THEN 'Synced with Cloud'
    ELSE FC.ZCLOUDLOCALSTATE
  END AS 'Face Crop Cloud Local State FC',
  CASE MEM.ZCLOUDLOCALSTATE
    WHEN 0 THEN '0-Not Synced with Cloud' 
    WHEN 1 THEN '1-Synced with Cloud'
    ELSE MEM.ZCLOUDLOCALSTATE
  END AS 'Memory Cloud Local State Memory',
  CASE SUGG.ZCLOUDLOCALSTATE
    WHEN 0 THEN '0-Not Synced with Cloud'
    WHEN 1 THEN '1-Syn with Cloud'
    ELSE SUGG.ZCLOUDLOCALSTATE
  END AS 'Suggestion Cloud Local State SUGG',
  CASE IR.ZCLOUDLOCALSTATE
    WHEN 0 THEN 'Not Synced / is in Cloud Share Album'
    WHEN 3 THEN 'Synced with Cloud'
    ELSE IR.ZCLOUDLOCALSTATE
  END AS 'Internal Resource Cloud Local State IR',
  CASE SUGG.ZSTATE
    WHEN 0 THEN '0-Unknown'
    WHEN 1 THEN '1-Unknown'
    ELSE SUGG.ZSTATE
  END AS 'Suggestion State',  
  CASE PER.ZCLOUDVERIFIEDTYPE
    WHEN 0 THEN '0-Unknown' 
    WHEN 1 THEN '1-Unknown'
    WHEN 2 THEN '2-Unknown'
    ELSE PER.ZCLOUDVERIFIEDTYPE
  END AS 'Cloud Verified Type Person',
  CASE CM.ZMOMENTSHARE
    WHEN 1 THEN 'Yes Cloud Master Moment Shared'
	WHEN 2 THEN 'Not Currently Synced Cloud Master Moment Shared'
    ELSE CM.ZMOMENTSHARE
  END AS 'Moment Share Cloud Master',
  CASE ZASSET.ZVISIBILITYSTATE
    WHEN 0 THEN 'Not in Cloud Shared Album'
    WHEN 2 THEN 'In Cloud Shared Album'
    ELSE ZASSET.ZVISIBILITYSTATE
  END AS 'Visibility State Asset',
  CASE DF.ZASSETVISIBLE
    WHEN 0 THEN '0-No'
    WHEN 1 THEN '1-Yes'  
    ELSE DF.ZASSETVISIBLE
  END AS 'Asset Visible DF',
  CASE ZASSET.ZCLOUDISMYASSET
    WHEN 1 THEN 'Yes File is My Asset'
    WHEN 0 THEN 'No File is not My Asset'
    ELSE 'NA'
  END AS 'File in Shared Album My Asset',
  CASE ZAddAssetAttr.ZSHARETYPE
    WHEN 0 THEN 'Not Cloud Master Moment Share'
    WHEN 1 THEN 'Is Cloud Master Moment Share'
    ELSE ZAddAssetAttr.ZSHARETYPE
  END AS 'Share Type CMM AddAssetAttr',
  CASE IR.ZCLOUDSOURCETYPE
    WHEN 0 THEN '0-Unknown'
    ELSE IR.ZCLOUDSOURCETYPE
  END AS 'Cloud Source Type Internal Resource',
  CASE IR.ZLOCALAVAILABILITY
    WHEN -1 THEN 'NA / Unknown'
    WHEN 1 THEN 'Yes'
    ELSE IR.ZLOCALAVAILABILITY
  END AS 'Local Avilability Internal Resource',
  CASE IR.ZREMOTEAVAILABILITY
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes - Available in Cloud Storage'
    ELSE IR.ZREMOTEAVAILABILITY
  END AS 'Remot Avilability Internal Resource',
  IR.ZREMOTEAVAILABILITYTARGET AS 'Remot Avilability Target Internal Resource',
  IR.ZSIDECARINDEX AS 'Side Car Index Key Internal Resource',
  CASE ZASSET.ZKIND
    WHEN 0 THEN 'Photo'
    WHEN 1 THEN 'Video'
  END AS 'Kind of Media Asset',
  CASE CM.ZFULLSIZEJPEGSOURCE
    WHEN 0 THEN 'No is a Video'
    WHEN 1 THEN 'Yes'
	ELSE CM.ZFULLSIZEJPEGSOURCE
  END AS 'Full Size Jpeg is Source Cloud Master',
  CM.ZCODECNAME AS 'Codec Name Cloud Master',
  ZCODEC.ZFOURCHARCODENAME AS 'Four Charcode Name Codec',
  CASE ZASSET.ZKINDSUBTYPE 
    WHEN 0 THEN 'Still Photo'
    WHEN 2 THEN 'Live Photo'
    WHEN 10 THEN 'SpringBoard Screenshot'
    WHEN 100 THEN 'Video'
    WHEN 101 THEN 'Slow-Mo Video'
    WHEN 102 THEN 'Time-lapse Video'
    ELSE ZASSET.ZKINDSUBTYPE
  END AS 'Kind Sub Type Asset',
  CASE IR.ZRESOURCETYPE
    WHEN 0 THEN '0-Photo'
    WHEN 1 THEN '1-Video'
    WHEN 3 THEN '3-Live Photo,'
	WHEN 5 THEN '5-Setting was Changed Prior to Capture'
    WHEN 6 THEN '6-Screenshot,'
    WHEN 13 THEN '13-Unknown'
    ELSE IR.ZRESOURCETYPE
  END AS 'Resource Type Internal Resource',
  CASE ZASSET.ZPLAYBACKSTYLE
    WHEN 1 THEN 'Still Photo'
    WHEN 3 THEN 'Live Photo'
    WHEN 4 THEN 'Video'
    ELSE ZASSET.ZPLAYBACKSTYLE
  END AS 'Playback Style Asset',
  CASE ZAddAssetAttr.ZCLOUDKINDSUBTYPE
    WHEN 0 THEN 'Still Photo'
    WHEN 2 THEN 'Live Photo'
    WHEN 3 THEN 'Screenshot'
    WHEN 100 THEN 'Video'
    WHEN 101 THEN 'Time-lapse'
    ELSE ZAddAssetAttr.ZCLOUDKINDSUBTYPE
  END AS 'Cloud Kind Sub Type AddAssetAttr',
  ZASSET.ZVIDEOCPDURATIONVALUE AS 'Live Photo Duration Asset',
  ZASSET.ZDURATION AS 'Video Duration Asset',
  ZASSET.ZPLAYBACKVARIATION AS 'Playback Variation Asset',
  CM.ZVIDEOFRAMERATE AS 'Video Frame Rate Cloud Master',
  ZAddAssetAttr.ZPENDINGVIEWCOUNT AS 'Pending View Count AddAssetAttr',
  ZAddAssetAttr.ZVIEWCOUNT AS 'View Count AddAssetAttr',
  ZAddAssetAttr.ZPENDINGPLAYCOUNT AS 'Pending Play Count AddAssetAttr',
  ZAddAssetAttr.ZPLAYCOUNT AS 'Play Count AddAssetAttr',
  ZAddAssetAttr.ZPENDINGSHARECOUNT AS 'Pending Share Count AddAssetAttr',
  ZAddAssetAttr.ZSHARECOUNT AS 'Share Count AddAssetAttr',
  ZAddAssetAttr.ZCLOUDSTATERECOVERYATTEMPTSCOUNT AS 'Cloud State Recovery Attempts Count',
  ZASSET.ZCLOUDDOWNLOADREQUESTS AS 'Cloud Download Requests Asset',
  DateTime(IR.ZCLOUDLASTONDEMANDDOWNLOADDATE + 978307200, 'UNIXEPOCH') AS 'Cloud Last OnDemand Download Date Internal Resource',
  DateTime(CLDRes.ZLASTONDEMANDDOWNLOADDATE + 978307200, 'UNIXEPOCH') AS 'Last OnDemand Download Date CLDRes',
  ZAddAssetAttr.ZUPLOADATTEMPTS AS 'Upload Attempts AddAssetAttr',
  CASE ZASSET.ZHASADJUSTMENTS
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
  END AS 'Has Adjustments Asset',
  DateTime(ZASSET.ZADJUSTMENTTIMESTAMP + 978307200, 'UNIXEPOCH') AS 'Adjustment Date Asset',
  DateTime(UA.ZADJUSTMENTTIMESTAMP + 978307200, 'UNIXEPOCH') AS 'Adjustment Timestamp UA',
  UA.ZADJUSTMENTFORMATIDENTIFIER AS 'Adjustment Format ID UA',
  UA.ZADJUSTMENTFORMATVERSION AS 'Adjustment Format Version UA', 
  UA.ZEDITORLOCALIZEDNAME AS 'Editor Localized Name UA',
  UA.ZADJUSTMENTBASEIMAGEFORMAT AS 'Adjustment Base Image Format UA',
  UA.ZADJUSTMENTRENDERTYPES AS 'Adjustment Render Types UA',
  CASE ZASSET.ZDERIVEDCAMERACAPTUREDEVICE
    WHEN 0 THEN 'Captured with Back Camera, Screenshot, Cloud, downloaded, Other'
    WHEN 1 THEN 'Captured with Front Camera'
    ELSE ZASSET.ZDERIVEDCAMERACAPTUREDEVICE
  END AS 'Derived Camera Capture Device Asset',
  CASE ZAddAssetAttr.ZCAMERACAPTUREDEVICE 
    WHEN 0 THEN 'Captured with Back Camera, Screenshot, Cloud, downloaded, Other'
    WHEN 1 THEN 'Captured with Front Camera'
    ELSE ZAddAssetAttr.ZCAMERACAPTUREDEVICE
  END AS 'Camera Capture Device AddAssetAttr',
  CASE ZAddAssetAttr.ZIMPORTEDBY
    WHEN 0 THEN 'Cloud / Other'
    WHEN 1 THEN 'Native Back Camera'
    WHEN 2 THEN 'Native Front Camera'
    WHEN 3 THEN 'Third Party App - Facebook, WhatsApp, Snapchat, Twitter, Screenshot made, Other'
    WHEN 4 THEN '4-Unknown'
    WHEN 5 THEN '5-Unknown'
    WHEN 6 THEN 'Third Party App - Facebook, Facebook-Mess, Reddit, Whisper, Other'
    WHEN 7 THEN '7-Unknown'
    WHEN 8 THEN 'Native App - SpringBoard, MessageCamera, Sharingd, Other'
    WHEN 9 THEN 'Native App - MobileSMS, Safari, Other'
    ELSE ZAddAssetAttr.ZIMPORTEDBY
  END AS 'Imported By AddAssetAttr',
  CASE CM.ZIMPORTEDBY
    WHEN 0 THEN 'Cloud / Other'
    WHEN 1 THEN 'Native Back Camera'
    WHEN 2 THEN 'Native Front Camera'
    WHEN 3 THEN 'Third Party App - Facebook, WhatsApp, Snapchat, Twitter, Screenshot made, Other'
    WHEN 4 THEN '4-Unknown'
    WHEN 5 THEN '5-Unknown'
    WHEN 6 THEN 'Third Party App - Facebook, Facebook-Mess, Reddit, Whisper, Other'
    WHEN 7 THEN '7-Unknown'
    WHEN 8 THEN 'Native App - SpringBoard, MessageCamera, Sharingd, Other'
    WHEN 9 THEN 'Native App - MobileSMS, Safari, Other'
    ELSE CM.ZIMPORTEDBY
  END AS 'Imported by Cloud Master',
  CMMD.ZDATA AS 'HEX Data Cloud Master MetaData',
  CASE ZAddAssetAttr.ZORIGINALORIENTATION
    WHEN 1 THEN 'Horizontal Camera (left) / Default for All Videos / Adjusted Photos'
    WHEN 2 THEN 'Horizontal Camera (right)'
    WHEN 3 THEN 'Horizontal Camera (right)'
    WHEN 4 THEN 'Horizontal Camera (left)'
    WHEN 5 THEN 'Vertical Camera (top)'
    WHEN 6 THEN 'Vertical Camera (top)'
    WHEN 7 Then 'Vertical Camera (bottom)'
    WHEN 8 THEN 'Vertical Camera (bottom)'
    ELSE ZASSET.ZORIENTATION
  END AS 'Orginal File Orientation Additonal Asset Attributes',
  CASE ZASSET.ZORIENTATION
    WHEN 1 THEN 'Horizontal Camera (left) / Default for All Videos / Adjusted Photos'
    WHEN 2 THEN 'Horizontal Camera (right)'
    WHEN 3 THEN 'Horizontal Camera (right)'
    WHEN 4 THEN 'Horizontal Camera (left)'
    WHEN 5 THEN 'Vertical Camera (top)'
    WHEN 6 THEN 'Vertical Camera (top)'
    WHEN 7 Then 'Vertical Camera (bottom)'
    WHEN 8 THEN 'Vertical Camera (bottom)'
    ELSE ZASSET.ZORIENTATION
  END AS 'File Orientation Asset',
  CASE CM.ZORIGINALORIENTATION
    WHEN 1 THEN 'Horizontal Camera (left) / Default for All Videos . Adjusted Photos'
    WHEN 2 THEN 'Horizontal Camera (right)'
    WHEN 3 THEN 'Horizontal Camera (right)'
    WHEN 4 THEN 'Horizontal Camera (left)'
    WHEN 5 THEN 'Vertical Camera (top)'
    WHEN 6 THEN 'Vertical Camera (top)'
    WHEN 7 Then 'Vertical Camera (bottom)'
    WHEN 8 THEN 'Vertical Camera (bottom)'
    ELSE CM.ZORIGINALORIENTATION
  END AS 'Original Orientation Cloud Master',
  CASE IR.ZORIENTATION
    WHEN 0 THEN 'Unknown'
    WHEN 1 THEN 'Horizontal Camera (left) / Default for All Videos / Adjusted Photos'
    WHEN 2 THEN 'Horizontal Camera (right)'
    WHEN 3 THEN 'Horizontal Camera (right)'
    WHEN 4 THEN 'Horizontal Camera (left)'
    WHEN 5 THEN 'Vertical Camera (top)'
    WHEN 6 THEN 'Vertical Camera (top)'
    WHEN 7 Then 'Vertical Camera (bottom)'
    WHEN 8 THEN 'Vertical Camera (bottom)'
    ELSE IR.ZORIENTATION
  END AS 'Orientation Internal Resource',
  CASE ZASSET.ZFAVORITE
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
  END AS 'Asset is Favorite Asset',
  CASE MEM.ZFAVORITE
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
  END AS 'Memory is Favorite Memory',
  CASE ZASSET.ZHIDDEN
    WHEN 0 THEN 'Not Hidden'
    WHEN 1 THEN 'Hidden'
    ELSE ZASSET.ZHIDDEN
  END AS 'Asset is Hidden File Asset',
  CASE DF.ZHIDDEN
    WHEN 0 THEN 'Not Hidden'
    WHEN 1 THEN 'Hidden'
    ELSE DF.ZHIDDEN
  END AS 'Detected Face Hidden File Detected Face',
  CASE ZASSET.ZTRASHEDSTATE
    WHEN 1 THEN 'In Trash'
    WHEN 0 THEN 'Not In Trash'
    ELSE ZASSET.ZTRASHEDSTATE
  END AS 'Asset on Devcie is in Trash',
  DateTime(ZASSET.ZTRASHEDDATE + 978307200, 'UNIXEPOCH') AS 'Trashed Date Asset',
  CASE DF.ZISINTRASH
    WHEN 1 THEN 'In Trash'
    WHEN 0 THEN 'Not In Trash'
    ELSE DF.ZISINTRASH
  END AS 'Detected Face is in Trash',
  CASE IR.ZTRASHEDSTATE
    WHEN 1 THEN 'In Trash'
    WHEN 0 THEN 'Not In Trash'
    ELSE IR.ZTRASHEDSTATE
  END AS 'Trash State Internal Resource',
  DateTime(IR.ZTRASHEDDATE + 978307200, 'UNIXEPOCH') AS 'Trashed Date Internal Resource',
  Case ZASSET.ZCLOUDDELETESTATE
    WHEN 0 THEN 'Not Deleted'
    WHEN 1 THEN 'Deleted'
    ELSE ZASSET.ZCLOUDDELETESTATE
  END AS 'Asset Cloud Delete State Asset',
  Case PER.ZCLOUDDELETESTATE
    WHEN 0 THEN 'Not Deleted'
    WHEN 1 THEN 'Deleted'
    ELSE PER.ZCLOUDDELETESTATE
  END AS 'Person Cloud Delete State Person',
    Case IR.ZCLOUDDELETESTATE
    WHEN 0 THEN 'Not Deleted'
    WHEN 1 THEN 'Deleted'
    ELSE IR.ZCLOUDDELETESTATE
  END AS 'Cloud Delete State Internal Resource',
  CASE FC.ZCLOUDDELETESTATE
    WHEN 0 THEN 'Not Deleted'
    WHEN 1 THEN 'Deleted'
    ELSE FC.ZCLOUDDELETESTATE
  END AS 'Cloud Delete State FC',
   CASE SUGG.ZCLOUDDELETESTATE
    WHEN 0 THEN '0-Not Deleted'
    WHEN 1 THEN '1-Deleted'
    ELSE SUGG.ZCLOUDDELETESTATE
  END AS 'Suggestion Cloud Delete State SUGG',
  CASE ZASSET.ZCLOUDISDELETABLE
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
    ELSE ZASSET.ZCLOUDISDELETABLE
  END AS 'Cloud File is Deletable Asset',
   Case MEM.ZCLOUDDELETESTATE
    WHEN 0 THEN '0-Not Deleted'
    WHEN 1 THEN '1-Deleted'
    ELSE MEM.ZCLOUDDELETESTATE
  END AS 'Cloud Delete State Memory',
  CASE ZAddAssetAttr.ZPTPTRASHEDSTATE
    WHEN 0 THEN '0-Unknown'
    WHEN 1 THEN '1-Unknown'
    ELSE ZAddAssetAttr.ZPTPTRASHEDSTATE
  END AS 'PTP Trashed State AddAssetAttr',
  CASE IR.ZPTPTRASHEDSTATE
    WHEN 0 THEN '0-Unknown'
    WHEN 1 THEN '1-Unknown'
    ELSE IR.ZPTPTRASHEDSTATE
  END AS 'PTP Trashed State Internal Resource',
  DateTime(ZASSET.ZCLOUDLASTVIEWEDCOMMENTDATE + 978307200, 'UNIXEPOCH') AS 'Cloud Last Viewed Comment Date Asset',
  Case ZASSET.ZCLOUDHASCOMMENTSBYME
    WHEN 1 THEN 'Yes Comments made'
    ELSE ZASSET.ZCLOUDHASCOMMENTSBYME
  END AS 'Cloud Comments by Me Asset',
  CASE ZASSET.ZCLOUDHASCOMMENTSCONVERSATION
    WHEN 1 THEN 'Yes Comments Conversation'
    ELSE ZASSET.ZCLOUDHASCOMMENTSCONVERSATION
  END AS 'Cloud Comments Conversation Asset',
  CASE CSC.ZISBATCHCOMMENT
    WHEN 1 THEN 'Yes'
    WHEN 0 THEN 'No'
    ELSE CSC.ZISBATCHCOMMENT
  END AS 'Cloud Shared Comment Is Batch Comment',
  CASE CSC.ZISCAPTION
    WHEN 1 THEN 'Yes'
    WHEN 0 THEN  'No'
    ELSE CSC.ZISCAPTION
  END AS 'Cloud Shared Comment Is Caption', 
  CASE CSC.ZISDELETABLE
    WHEN 1 THEN 'Yes'
    WHEN 0 THEN 'No'
    ELSE CSC.ZISDELETABLE
  END AS 'Cloud Shared Comment Is Deletable',
  CASE CSC.ZISLIKE
    WHEN 1 THEN 'Yes'
    WHEN 0 THEN 'No'
    ELSE CSC.ZISLIKE
  END AS 'Cloud Shared Comment Is Liked',
  CASE CSC.ZISMYCOMMENT
    WHEN 1 THEN 'Yes'
    WHEN 0 THEN 'No'
    ELSE CSC.ZISMYCOMMENT
  END AS 'Cloud Shared Comment Is My Comment',
  DateTime(CSC.ZCOMMENTCLIENTDATE + 978307200, 'UNIXEPOCH') AS 'Comment Client Date Cloud Shared Comment',
  DateTime(CSC.ZCOMMENTDATE + 978307200, 'UNIXEPOCH') AS 'Comment Date Cloud Shared Comment',
  CSC.ZCOMMENTTEXT AS 'Comment Text Cloud Shared Comment',
  CSC.ZCOMMENTTYPE AS 'Comment Type Cloud Shared Comment',
  CSC.ZCOMMENTERHASHEDPERSONID AS 'Commenter Hashed Person ID Cloud Shared Comment',
  ZAddAssetAttr.ZTITLE AS 'Title - Comments via Web Cloud - AddAssetAttr',
  ZAddAssetAttr.ZACCESSIBILITYDESCRIPTION AS 'Accessibility Description AddAssetAttr',
  AssetDes.ZLONGDESCRIPTION AS 'Long Description Asset Description',
  CASE ZASSET.ZCLOUDHASUNSEENCOMMENTS
    WHEN 0 THEN 'No Unseen Comments'
    WHEN 1 THEN 'Yes has Unseen Comments'
    ELSE ZASSET.ZCLOUDHASUNSEENCOMMENTS
  END AS 'Cloud Has Unseen Comments Asset',
  CASE ZAddAssetAttr.ZALLOWEDFORANALYSIS
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
    ELSE ZAddAssetAttr.ZALLOWEDFORANALYSIS
  END AS 'Allowed for Analysis AddAssetAttr',
  DateTime(ZAddAssetAttr.ZSCENEANALYSISTIMESTAMP + 978307200, 'UNIXEPOCH') AS 'Scene Analysis Date AddAssetAttr',
  DateTime(ZASSET.ZANALYSISSTATEMODIFICATIONDATE + 978307200, 'UNIXEPOCH') AS 'Analysis State Mod. Date Asset',
  DateTime(MedAnalyAsset.ZMEDIAANALYSISTIMESTAMP + 978307200, 'UNIXEPOCH') AS 'Media Analysis Timestamp MedAnalyAsset',
  SceneP.ZDATA AS 'HEX NSKeyed Plist Scene Print Data SceneP',
  CASE ZASSET.ZLATITUDE
    WHEN -180.0 THEN '0'
    ELSE ZASSET.ZLATITUDE
  END AS 'Latitude Asset',
  CASE ZASSET.ZLONGITUDE
    WHEN -180.0 THEN '0'
    ELSE ZASSET.ZLONGITUDE
  END AS 'Longitude Asset',
  CASE ZAddAssetAttr.ZGPSHORIZONTALACCURACY
    WHEN -1.0 THEN 'Not Valid'
    ELSE ZAddAssetAttr.ZGPSHORIZONTALACCURACY
  END AS 'Horizontal Accuracy AddAssetAttr',
  CASE ZAddAssetAttr.ZREVERSELOCATIONDATAISVALID
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
  END AS 'Is Reverse Location Data Valid AddAssetAttr',
  ZAddAssetAttr.ZREVERSELOCATIONDATA AS 'HEX NSKeyed Plist Org File Reverse Location Data AddAssetAttr',
  ZASSET.ZLOCATIONDATA AS 'HEX Location Data Asset',
  CASE ZAddAssetAttr.ZSHIFTEDLOCATIONISVALID
    WHEN 0 THEN 'No'
    WHEN 1 THEN 'Yes'
  END AS 'Is Shifted Location Valid AddAssetAttr',
  ZAddAssetAttr.ZSHIFTEDLOCATIONDATA AS 'Shifted Location Data AddAssetAttr',
